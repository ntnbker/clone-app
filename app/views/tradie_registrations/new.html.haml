=bootstrap_nested_form_for(@user) do |f|
  =f.email_field :email, hide_label: false
  =f.password_field :password, hide_label: false
  =f.password_field :password_confirmation


  =f.fields_for :trady do |tradie|
    =tradie.text_field :name
    =tradie.text_field :company_name
    =tradie.phone_field :mobile
    / =admin.hidden_field :maintenance_request_reference_id , value: @maintenance_request_reference_id


  =f.submit "Sign Up"

  / =link_to "Back", edit_agency_registration_path(agency_id:@agency.id)