.container.well#login
  = bootstrap_form_tag url: '/menulogin' do |f| 
    = f.select(:role_picked, [["Agent","Agent"],["Agency Admin","AgencyAdmin"],["Tenant","Tenant"],["Trady","Trady"],["Landlord","Landlord"]], {prompt:"Select Role" })
    = f.email_field :email, placeholder: 'name@example.com'
    = f.password_field :password
    = link_to "Forgot your password?", new_password_reset_path
    = f.submit "Log In", class:"button-primary green"
